<%- include('../partials/header') %>

    <ul>
      <% skills.forEach(function(s) { %>
        <li>
          <form
            action="/skills/<%= s.id %>?_method=DELETE"
            method="POST"
            class="delete-form"
          >
            <button type="submit">X</button>
          </form>  
          <a href="/skills/<%= s.id %>"><%= s.skill %></a>
          - 
          <%= s.status ? 'extra practice' : 'a ton of extra practice' %>
        </li>
      <% }); %>
    </ul>
    <a href="/skills/new">Add Skill</a>

<%- include('../partials/footer') %>